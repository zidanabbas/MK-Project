// jsconfig.json
{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    // Next.js merekomendasikan ini untuk jsconfig.json di proyek React
    "jsx": "preserve"
  },
  // Sertakan semua file yang relevan agar path alias bekerja
  "include": ["**/*.js", "**/*.jsx", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
